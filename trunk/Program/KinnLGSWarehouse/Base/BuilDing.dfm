  object udsBuilding: TKadoUpdateSql
    ModifySQL.Strings = (
      'Update Building'
      ' Set '
      '      jobno               = :jobno,'
      '      bd_located          = :bd_located,'
      '      bd_landtype         = :bd_landtype,'
      '      bd_housing          = :bd_housing,'
      '      bd_parcelarea       = :bd_parcelarea,'
      '      bd_assessmentarea   = :bd_assessmentarea,'
      '      bd_landarea         = :bd_landarea,'
      '      bd_constructionarea = :bd_constructionarea,'
      '      bd_property         = :bd_property,'
      '      bd_housecusid       = :bd_housecusid,'
      '      bd_housecusname     = :bd_housecusname,'
      '      bd_housetype        = :bd_housetype,'
      '      bd_buildyear        = :bd_buildyear,'
      '      idleid              = :idleid,'
      '      idleday             = :idleday,'
      '      houseno             = :houseno,'
      '      bd_floor            = :bd_floor,'
      '      bd_area             = :bd_area,'
      '      CreateName          = :CreateName,'
      '      Createdate          = :Createdate,'
      '      modifyname          = :modifyname,'
      '      modifydate          = :modifydate,'
      '      opid                = :opid,'
      '      opname              = :opname,'
      '      userid              = :userid,'
      '      username            = :username,'
      '      iuserid             = :iuserid,'
      '      iusername           = :iusername,'
      '      remark              = :remark'
      ' Where  jobno  = :OLD_jobno')
    InsertSQL.Strings = (
      ' Insert Into Building'
      '  (jobno,bd_located,bd_landtype,bd_housing,bd_parcelarea,'
      
        '   bd_assessmentarea,bd_landarea,bd_constructionarea,bd_property' +
        ',bd_housecusid,'
      '   bd_housecusname,bd_housetype,bd_buildyear,idleid,idleday,'
      '   houseno,bd_floor,bd_area,CreateName,Createdate,'
      '   modifyname,modifydate,opid,opname,userid,'
      '   username,iuserid,iusername,remark)'
      ' Values '
      '  (:jobno,:bd_located,:bd_landtype,:bd_housing,:bd_parcelarea,'
      
        '   :bd_assessmentarea,:bd_landarea,:bd_constructionarea,:bd_prop' +
        'erty,:bd_housecusid,'
      
        '   :bd_housecusname,:bd_housetype,:bd_buildyear,:idleid,:idleday' +
        ','
      '   :houseno,:bd_floor,:bd_area,:CreateName,:Createdate,'
      '   :modifyname,:modifydate,:opid,:opname,:userid,'
      '   :username,:iuserid,:iusername,:remark)')
    DeleteSQL.Strings = (
      ' Delete '
      ' From Building'
      ' Where  jobno  = :OLD_jobno')
    Separator = '!'
    Left = 431
    Top = 188
  end
